{
    "name": "kickoff-react",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "concurrently -r 'npm run compile-scss' 'react-scripts start'",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "compile-scss": "node-sass ./src/styles/style.scss -w -o ./src/styles"
    },
    "dependencies": {
        "axios": "^0.15.3",
        "axios-mock-adapter": "^1.8.1",
        "history": "^4.6.1",
        "immutable": "3.8.1",
        "key-mirror": "1.0.1",
        "moment": "^2.18.1",
        "normalizr": "^3.2.2",
        "qs": "^6.4.0",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-helmet": "^5.0.0",
        "react-redux": "5.0.3",
        "react-router": "^3.0.0",
        "redux": "3.6.0",
        "redux-actions": "^2.0.1",
        "redux-logger": "^2.10.2",
        "redux-persist": "^4.5.0",
        "redux-persist-transform-immutable": "^4.2.0",
        "redux-saga": "^0.14.3",
        "redux-thunk": "2.2.0",
        "underscore": "^1.8.3",
        "underscore.string": "^3.3.4"
    },
    "devDependencies": {
        "babel-eslint": "^7.2.0",
        "concurrently": "^3.4.0",
        "eslint": "^3.18.0",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-plugin-flowtype": "^2.30.4",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.10.3",
        "node-sass": "^4.5.1",
        "react-scripts": "0.9.5",
        "surge": "^0.19.0"
    }
}
